{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jjrevset",
      "camelcase": "JjRevset",
      "scope": "jj.revset",
      "file-types": [
        "jjrevset"
      ],
      "injection-regex": "^jjrevset$",
      "class-name": "TreeSitterJjRevset"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for jj revsets",
    "authors": [
      {
        "name": "Bryce Berger",
        "email": "bryce.z.berger@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/bryceberger/tree-siter-jjrevset"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
